{
  "name": "primo",
  "description": "Car rentals and Selling",
  "version": "1.0.0",
  "author": "Gilles Kagarama",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./build/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build src/",
    "build": "npm run clean && npm run build-babel",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:generate:table": "./node_modules/.bin/sequelize sequelize db:migration:generate --name",
    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "db:migrate:undo:all": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "db:seed:undo:all": "./node_modules/.bin/sequelize db:seed:undo:all"
  },
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/node": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "@babel/runtime": "^7.21.0",
    "@sendgrid/mail": "^6.4.0",
    "axios": "^1.3.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "celebrate": "^15.0.0",
    "cloudinary": "^1.14.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^6.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.1",
    "firebase-admin": "^11.5.0",
    "generate-otp": "^1.0.6",
    "global": "^4.4.0",
    "http-errors": "^1.7.2",
    "joi": "^17.8.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "methods": "^1.1.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "multer-storage-cloudinary": "^2.2.1",
    "node-mailjet": "^3.3.4",
    "open": "^6.3.0",
    "perf_hooks": "^0.0.1",
    "pg": "8.7.1",
    "pg-hstore": "2.3.4",
    "request": "^2.88.2",
    "sequelize": "5.22.3",
    "sequelize-easy-query": "^1.1.0",
    "slug": "^1.1.0",
    "socket.io": "^4.6.1",
    "swagger": "^0.7.5",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "uniqid": "^5.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "nodemon": "^1.19.1"
  }
}
